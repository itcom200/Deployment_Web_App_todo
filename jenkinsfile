pipeline {
    agent any
    
    stages {
        stage("Get Code Bas testing") {
            steps {
                git "https://github.com/itcom200/django-example-todo.git"
            }
        }
        stage("Install Library Bas testing"){
            steps{
                bat "python --version"
            }
        }
        stage("Unit test bas testing"){
            steps{
                bat "python tests/TestNumber.py"
            }
        }
    }
    post{
        always{
            junit "test-reports/*.xml"
        }
    }
}
